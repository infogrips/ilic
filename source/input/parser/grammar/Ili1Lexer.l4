//
// start of LEXER
//

lexer grammar Ili1Lexer;

//
// whitespace & comments
//

WS
   : (' ' | '\t' | '\f' | ('\r'? '\n'))+
   -> skip;

SL_COMMENT
   : '!!'
   (~'@')
   (~('\n'|'\r'|'\uFFFF'))*
   ('\n' | ('\r' ( '\n' )?)|'\uFFFF')
   -> skip
  ;
  
ILI_DOC
    : '/**' .*? '*/' -> skip
    ;

ILI_OPTION
    : '!!@' .*? ('\n' | ('\r' ( '\n' )?)|'\uFFFF') -> skip
    ;
    
ILI_BLOCKCOMMENT
    : '/*' .*? '*/' -> skip
    ;
    
