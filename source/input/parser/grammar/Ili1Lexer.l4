//
// complex tokens
//

EXPLANATION
    : '//' .*? '//'
    ;

ESC
   : '\\' ( '"' | '\\' | 'u' HEXDIGIT HEXDIGIT HEXDIGIT HEXDIGIT )
   ;

STRING
   : '"' ( ESC | ~('"' | '\\'))* '"'
   ;

fragment DIGIT
   : '0' .. '9'
   ;

fragment LETTER
   :   'a' .. 'z' | 'A' .. 'Z'
   ;

POSNUMBER
   : (DIGIT)+
   ;

NUMBER
   : ('+'|'-')? POSNUMBER
   ;

DEC
   : NUMBER DOT POSNUMBER SCALING?
   ;

SCALING
   : ('E'|'e') NUMBER
   ;

DECIMAL
   : (('+' |'-')? '0' DOT (('1'..'9' (POSNUMBER)?)
   | ('0')+) ('e' | 'E'))
   | NUMBER (DOT POSNUMBER)?
   ;      

fragment HEXDIGIT
   : DIGIT
   | [a-f]
   | [A-F]
   ;

HEXNUMBER
   : '0' ('x' | 'X') (HEXDIGIT)+
   ;

NAME
   : LETTER (LETTER | '_' | DIGIT)*
   ;
